        const handlePage = (event, currentPage, totalPages) => {
            let pageNumber = event.target.innerText;
            console.log("Page ", currentPage);
            if (pageNumber == "Previous") {
                if (currentPage == '1') {
                    pageNumber = parseInt(currentPage);
                } else {
                    pageNumber = parseInt(currentPage) - 1;
                }
            }
            else if (pageNumber == "Next") {
                if (currentPage == totalPages) {
                    pageNumber = parseInt(totalPages);
                } else {
                    pageNumber = parseInt(currentPage) + 1;
                }
            }
            console.log("Page num", pageNumber);

            const url = new URL(window.location.href);
            url.searchParams.set('page', pageNumber);
            window.location.href = url.toString();

