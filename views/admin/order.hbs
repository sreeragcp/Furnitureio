    <script>
        // Wait for the document to be fully loaded
        document.addEventListener("DOMContentLoaded", function () {
            // Get all address IDs
            var addressIds = document.getElementsByClassName("address-id");

            // Attach click event listener to each address ID
            Array.from(addressIds).forEach(function (addressId) {
                addressId.addEventListener("click", function () {
                    // Toggle the visibility of the dropdown content
                    var dropdownContent = addressId.nextElementSibling;
                    dropdownContent.style.display = (dropdownContent.style.display === "block") ? "none" : "block";
                });
            });
        });
    </script>

